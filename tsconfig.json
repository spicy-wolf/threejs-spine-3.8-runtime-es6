{
	"compilerOptions": {
		"module": "none",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"outFile": "build/spine-threejs-es6.js",
		"sourceMap": true,
		"declaration": true,
		"target": "ES6",
		"skipLibCheck": true,
	},
	"include": [
		"core/src/**/*",
		"threejs/src/**/*"
	],
	"exclude": [
		"build/**/*",
		"node_modules/**/*"
	]
}